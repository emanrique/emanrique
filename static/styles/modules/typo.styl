@css{

  @font-face {
    font-family: 'Titillium Web', sans-serif;
    src: url('http://fonts.googleapis.com/css?family=Titillium+Web:700,600,400');
    font-weight: normal;
    font-style: normal;
  }
  
  @font-face {
    font-family: 'Bree Serif', sans-serif;
    src: url('http://fonts.googleapis.com/css?family=Bree+Serif');
    font-weight: normal;
    font-style: normal;
  }

  @font-face {
    font-family: 'League Gothic';
    src: url('../typos/leaguegothic.eot');
    src: url('../typos/leaguegothic?#iefix') format('embeded-opentype'),
         url('../typos/leaguegothic.woff') format('woff'),
         url('../typos/leaguegothic.ttf')  format('truetype'),
         url('../typos/leaguegothic.svg') format('svg');
    font-weight: normal;
    font-style: normal;
  }
}