@css{
  @font-face {
    font-family: 'DroidSans';
    src: url('../typos/droidsans.eot');
    src: url('../typos/droidsans.eot') format('embedded-opentype'),
         url('../typos/droidsans.woff') format('woff'),
         url('../typos/droidsans.ttf') format('truetype'),
         url('../typos/droidsans.svg#DroidSans') format('svg');
    font-weight: normal;
    font-style: normal;
  }
  @font-face {
    font-family: 'flexslider-icon';
    src:url('../typos/flexslider-icon.eot');
    src:url('../typos/flexslider-icon.eot?#iefix') format('embedded-opentype'),
        url('../typos/flexslider-icon.woff') format('woff'),
        url('../typos/flexslider-icon.ttf') format('truetype'),
        url('../typos/flexslider-icon.svg#flexslider-icon') format('svg');
    font-weight: normal;
    font-style: normal;
  }
  @font-face {
    font-family: 'arrows';
    src: url('../typos/arrows.eot');
    src: url('../typos/arrows?#iefix') format('embeded-opentype'),
         url('../typos/arrows.woff') format('woff'),
         url('../typos/arrows.ttf')  format('truetype'),
         url('../typos/arrows.svg') format('svg');
    font-weight: normal;
    font-style: normal;
  }
}